{
  "name": "punk",
  "productName": "Punk",
  "version": "0.2.1",
  "description": "Alternative cross-platform Steam client for desktop",
  "main": "dist/js/main.js",
  "author": {
    "name": "Michael Scholtz",
    "email": "michael.scholtz@outlook.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/scholtzm/punk.git"
  },
  "scripts": {
    "build": "node_modules/.bin/gulp",
    "clean": "node_modules/.bin/gulp clean",
    "lint": "node_modules/.bin/eslint --ignore-path .gitignore .",
    "lint:fix": "node_modules/.bin/eslint --ignore-path .gitignore . --fix",
    "start": "node_modules/.bin/electron .",
    "watch": "node_modules/.bin/gulp watch",
    "package-osx": "npm run build && node scripts/package.js",
    "package-win": "npm run build && node scripts/package.js --win32"
  },
  "dependencies": {
    "babel-preset-react": "^6.11.1",
    "classnames": "^2.2.5",
