    "eslint-plugin-react": "^6.2.2",
    "flux": "^3.0.0",
    "font-awesome": "^4.6.3",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-rimraf": "^0.2.0",
    "gulp-sass": "^2.3.2",
    "mkdirp": "^0.5.1",
    "moment": "^2.15.0",
    "node-notifier": "^4.6.1",
    "object-assign": "^4.1.0",
    "photon": "github:connors/photon#5a1a8ef12fa9e16559ac090baef667707399a1c3",
